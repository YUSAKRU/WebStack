{
  "name": "laravel-app",
  "description": "A Laravel application",
  "type": "project",
  "require": {
    "php": "^7.3|^8.0",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0",
    "laravel/framework": "^8.0",
    "laravel/tinker": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.4",
    "nunomaduro/collision": "^5.0"
  },
  "scripts": {
    "test": "phpunit",
    "lint": "phpcs",
    "format": "phpcbf"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}